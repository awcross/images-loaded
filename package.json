{
  "name": "images-loaded",
  "version": "0.0.0",
  "description": "Wait for images to load using promises",
  "license": "MIT",
  "repository": "awcross/images-loaded",
  "author": {
    "name": "Alex Cross",
    "email": "crossaw@gmail.com",
    "url": "https://alexcross.io"
  },
  "engines": {
